{
    "name": "Krunker Market Ping",
    "short_name": "Market Ping",
    "version": "1.0",
    "description": "This allows you to see your ping on krunker market.",
    "permissions": [
        "tabs",
        "https://krunker.io/*"],
    "author": "hitthemoney and yeehow",
      "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "icon-16.png",
          "32": "icon-32.png",
          "48": "icon-48.png",
          "128": "icon-128.png"
        },
        "default_title": "Krunker Market Ping"
      },
      "icons": {
        "16": "icon-16.png",
        "32": "icon-32.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
      },
    "manifest_version": 2
  }
